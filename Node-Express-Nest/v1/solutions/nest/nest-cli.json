{
  "monorepo": true,
  "root": "apps/main",
  "sourceRoot": "apps/main/src",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/main/tsconfig.app.json"
  },
  "projects": {
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "nest-basics": {
      "type": "application",
      "root": "apps/nest-basics",
      "entryFile": "main",
      "sourceRoot": "apps/nest-basics/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-basics/tsconfig.app.json"
      }
    }
  }
}